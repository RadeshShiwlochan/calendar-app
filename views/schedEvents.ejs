<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./partials/head %>
</head>

<body>
	 <header>
        <% include ./partials/header %>
    </header>
    <div class="container">
    	<div class="row">
    		<div class="col-md-12">
    			 <h1 class="centerHeading"> All Events in your Calendar </h1>
    		</div>
    	</div>
    </div>
    <div class="container">
    	<div class="jumbotron">
	    	<div class="row">
	    		<div class="col-md-12">
	    			<% for ( var event in allEvents ) { %>
	    			    <% for ( var x = 0; x < allEvents[event].length; ++x ) { %>
			    			<h3 class="addPadding"> 
			    				<%= allEvents[event][x]['start'] %>
			    				-     
			    				<%= allEvents[event][x]['end'] %> 
			    				<%= allEvents[event][x]['description'] %> 
			    			</h3>
			    			<hr>
	    			    <% } %>
	    			<% } %>
	    		</div>
	    	</div>
        </div>
    </div>
    <footer> <% include ./partials/footer %> </footer>
</body>
</html>